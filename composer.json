{
  "name": "neoisrecursive/statamic-storage-info",
  "authors": [
    {
      "name": "Neo Lejondahl",
      "email": "neo.leijondahl@gmail.com"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Neoisrecursive\\StorageInfo\\": "src"
    }
  },
  "extra": {
    "download-dist": [
      {
        "url": "https://github.com/neoisrecursive/statamic-storage-info/releases/download/{$version}/dist.tar.gz",
        "path": "resources/dist"
      }
    ],
    "statamic": {
      "name": "Statamic storage Info",
      "description": "A widget that displays some nice to know info about selected asset containers"
    },
    "laravel": {
      "providers": [
        "Neoisrecursive\\StorageInfo\\ServiceProvider"
      ]
    }
  },
  "require": {
    "statamic/cms": "^4.4",
    "pixelfear/composer-dist-plugin": "^0.1.5"
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  },
  "require-dev": {
    "laravel/pint": "^1.10"
  }
}
